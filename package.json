{
  "name": "Helia",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release-dev": "react-native run-android --variant=devrelease",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "prod:setup": "ENV=production ./envscript.sh",
    "dev:setup": "ENV=development ./envscript.sh",
    "android:setup-dev": "yarn dev:setup && ENVFILE=configs/development/.env",
    "android:setup-prod": "yarn prod:setup && ENVFILE=configs/production/.env",
    "dev:android": "yarn android:setup-dev && yarn android",
    "dev:android-release": "yarn android:setup-dev && yarn android:release-dev",
    "dev:apk": "yarn dev:setup && cd android && ENVFILE=configs/development/.env ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "prod:apk": "yarn dev:setup && cd android && ENVFILE=configs/development/.env ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "prod:aab": "yarn prod:setup && cd android && ENVFILE=configs/development/.env ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "prod:android": "yarn prod:setup && ENVFILE=configs/production/.env npx react-native run-android",
    "prod:android-release": "yarn android:setup-prod && yarn android:release",
    "dev:ios": "yarn dev:setup && npx yarn ios --scheme 'Staging_OfficeManager'",
    "prod:ios": "yarn prod:setup && npx yarn ios --scheme 'OfficeManager'",
    "clean_build": "rm -rf ios/build android/app/build",
    "pod": "cd ios && pod install && cd ..",
    "pod:reinstall": "(rm -rf ios/Pods; rm -rf ios/Podfile.lock) && yarn pod",
    "cleanAll": "watchman watch-del-all && rm -rf yarn.lock package-lock.json node_modules ios/Pods ios/Podfile.lock android/app/build && yarn install && cd ios && pod update && cd .. && yarn start -- --reset-cache",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-firebase/app": "^17.3.0",
    "@react-native-firebase/messaging": "^17.3.0",
    "@react-navigation/bottom-tabs": "^6.5.5",
    "@react-navigation/native": "^6.1.4",
    "@react-navigation/native-stack": "^6.9.10",
    "@react-navigation/stack": "^6.3.14",
    "@reduxjs/toolkit": "^1.9.2",
    "@shopify/flash-list": "^1.4.1",
    "async-mutex": "^0.4.0",
    "i18next": "^22.4.10",
    "lodash.keys": "^4.2.0",
    "react": "18.1.0",
    "react-i18next": "^12.1.5",
    "react-native": "0.70.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-pager-view": "^6.1.4",
    "react-native-permissions": "^3.6.1",
    "react-native-reanimated": "^2.14.4",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^13.8.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash.keys": "^4.2.7",
    "@types/react-native": "^0.69.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
